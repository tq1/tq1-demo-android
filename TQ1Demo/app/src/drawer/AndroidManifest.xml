<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.taqtile.tq1demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".home.presentation.activity.HomeActivity"
            android:label="Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".productlist.presentation.activity.ProductListActivity"
            android:label="Product List">
        </activity>

        <activity
            android:name=".notificationlist.presentation.activity.NotificationListActivity"
            android:label="Notification List">
        </activity>

        <activity
            android:name=".profilenotlogged.presentation.activity.ProfileNotLoggedActivity"
            android:label="Profile">
        </activity>

        <activity
            android:name=".triggerlist.presentation.activity.TriggerListActivity"
            android:label="Trigger List">
        </activity>

        <activity
            android:name=".filter.presentation.activity.FilterActivity"
            android:label="Filters">
        </activity>

        <activity
            android:name=".signin.presentation.activity.SignInActivity"
            android:label="Sign In">
        </activity>


    </application>

</manifest>